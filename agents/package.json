  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/density-clustering": "^1.3.1",
    "@types/html-to-text": "^9.0.1",
    "@types/metascraper": "^5.14.1",
    "@types/node": "^20.12.7",
