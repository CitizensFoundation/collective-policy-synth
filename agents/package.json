    "@langchain/openai": "^0.0.28",
    "aws-sdk": "^2.1608.0",
    "axios": "^1.6.8",
    "bullmq": "^5.7.6",
    "density-clustering": "^1.3.0",
    "gpt-tokenizer": "^2.1.1",
    "html-get": "^2.16.3",
    "html-to-text": "^9.0.5",
    "ioredis": "^5.4.1",
    "jsonrepair": "^3.7.0",
    "mammoth": "^1.7.2",
    "metascraper": "^5.45.5",
    "metascraper-author": "^5.45.0",
    "metascraper-clearbit": "^5.45.0",
    "metascraper-date": "^5.45.0",
    "metascraper-description": "^5.45.0",
    "metascraper-image": "^5.45.0",
    "metascraper-logo": "^5.45.0",
    "metascraper-publisher": "^5.45.0",
    "metascraper-title": "^5.45.0",
    "metascraper-url": "^5.45.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.46.0",
    "pdf-parse": "^1.1.1",
    "pdf2json": "^3.0.4",
    "pdfjs-dist": "^3.8.162",
    "pdfreader": "^3.0.0",
    "pg": "^8.11.5",
    "puppeteer": "22.7.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "tiktoken": "^1.0.15",
    "weaviate-ts-client": "^2.1.1",
    "winston": "^3.13.0",
    "ws": "^8.16.0",
    "xlsx": "^0.18.5"
  }
